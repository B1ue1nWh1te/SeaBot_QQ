# 当前的唤醒方式为"SeaBot-命令 参数(可选)"
COMMAND_START=["-"] 
COMMAND_SEP=[" "]

# 消息等待时间
SESSION_EXPIRE_TIMEOUT=15 

# 需要发送定时消息的群
TIMINIG_GROUP_ID=[""] 