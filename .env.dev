# Windows下设为127.0.0.1 Linux下设为0.0.0.0
HOST=0.0.0.0 

# 端口
PORT=8080

# 开启调试模式
DEBUG=true

# 设置时区
APSCHEDULER_AUTOSTART=true
APSCHEDULER_CONFIG={"apscheduler.timezone": "Asia/Shanghai"}

# 机器人的昵称
NICKNAME=["SeaBot"] 

# 机器人管理员的QQ号
SUPERUSERS=[""] 

# 设置命令与参数的分隔符 当前的唤醒方式为"SeaBot-命令 参数(可选)"
COMMAND_START=["-"] 
COMMAND_SEP=[" "]

# 等待二级指令回复的时长
SESSION_EXPIRE_TIMEOUT=15 
